{
  "name": "vitepress-blog",
  "description": "Blog",
  "keywords": [
    "blog",
    "vitepress",
    "vue",
    "pnpm"
  ],
  "author": "zhixiangyao <blueskyzx@icloud.com> <https://github.com/zhixiangyao>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhixiangyao/vitepress-blog.git"
  },
  "homepage": "https://github.com/zhixiangyao/vitepress-blog",
  "scripts": {
    "lint:all": "npm run lint:eslint && npm run lint:prettier && npm run lint:stylelint",
    "lint:prettier": "prettier --cache --write --loglevel warn \"packages/**/*.{json,ts,tsx,css,vue,html,md}\"",
    "lint:eslint": "eslint --fix \"packages/**/*.{vue,ts,tsx}\" --cache --cache-location node_modules/.cache/eslint/",
    "lint:stylelint": "stylelint --fix \"packages/**/*.{vue,postcss,css}\" --cache --cache-location node_modules/.cache/stylelint/"
  },
  "devDependencies": {
    "@algolia/client-search": "^4.14.2",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.6.0",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "stylelint": "^14.14.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.8.4",
    "vitepress": "1.0.0-alpha.29",
    "vue": "^3.2.45"
  },
  "engines": {
    "node": ">=16.17.0",
    "pnpm": ">=7.17.0"
  }
}
