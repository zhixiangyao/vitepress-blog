{
  "name": "vitepress-blog",
  "description": "Blog",
  "keywords": [
    "blog",
    "vitepress",
    "vue",
    "pnpm"
  ],
  "author": "zhixiangyao <blueskyzx@icloud.com> <https://github.com/zhixiangyao>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhixiangyao/vitepress-blog.git"
  },
  "homepage": "https://github.com/zhixiangyao/vitepress-blog",
  "scripts": {
    "lint:all": "npm run lint:eslint && npm run lint:prettier && npm run lint:stylelint",
    "lint:eslint": "eslint --fix \"packages/**/*.{vue,ts,tsx}\" --cache --cache-location node_modules/.cache/eslint/",
    "lint:prettier": "prettier --cache --write --log-level warn \"packages/**/*.{json,ts,tsx,css,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix \"packages/**/*.{vue,postcss,css}\" --cache --cache-location node_modules/.cache/stylelint/"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "@typescript-eslint/parser": "6.2.1",
    "eslint": "8.46.0",
    "eslint-config-prettier": "8.9.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-vue": "9.16.1",
    "postcss-html": "1.5.0",
    "prettier": "3.0.0",
    "stylelint": "15.10.2",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard": "34.0.0",
    "stylelint-order": "6.0.3",
    "typescript": "5.1.6",
    "vitepress": "1.0.0-beta.7",
    "vue": "3.3.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  }
}
