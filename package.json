{
  "name": "vitepress-blog",
  "description": "Blog",
  "keywords": [
    "blog",
    "vitepress",
    "vue",
    "pnpm"
  ],
  "author": "zhixiangyao <blueskyzx@icloud.com> <https://github.com/zhixiangyao>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhixiangyao/vitepress-blog.git"
  },
  "homepage": "https://github.com/zhixiangyao/vitepress-blog",
  "scripts": {
    "lint:all": "npm run lint:eslint && npm run lint:prettier && npm run lint:stylelint",
    "lint:prettier": "prettier --cache --write --loglevel warn \"packages/**/*.{json,ts,tsx,css,vue,html,md}\"",
    "lint:eslint": "eslint --fix \"packages/**/*.{vue,ts,tsx}\" --cache --cache-location node_modules/.cache/eslint/",
    "lint:stylelint": "stylelint --fix \"packages/**/*.{vue,postcss,css}\" --cache --cache-location node_modules/.cache/stylelint/"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.10.0",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.7",
    "stylelint": "^15.3.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^31.0.0",
    "stylelint-order": "^6.0.3",
    "typescript": "^5.0.2",
    "vitepress": "1.0.0-alpha.61",
    "vue": "^3.2.47"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  },
  "engines": {
    "node": ">=16.17.0",
    "pnpm": ">=7.17.0"
  }
}
